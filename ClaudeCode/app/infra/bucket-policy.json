{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowLambdaToGetObject",
      "Effect": "Allow",
      "Principal": {
        "Service": "lambda.amazonaws.com"
      },
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::recipe-app-test-bucket/lambda.zip"
    },
    {
      "Sid": "AllowUserAccess",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::111460050170:user/nakao"
      },
      "Action": [
        "s3:GetObject",
        "s3:PutObject"
      ],
      "Resource": "arn:aws:s3:::recipe-app-test-bucket/*"
    }
  ]
}
